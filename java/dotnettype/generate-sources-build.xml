<project name="generate-sources">
    <echo message="Running protoc ..."/>
    <mkdir dir="${generated.sources.dir}"/>
    <exec executable="${protoc}">
        <arg value="--java_out=${generated.sources.dir}"/>
        <arg value="--proto_path=${protobuf.source.dir}"/>
        <arg value="--proto_path=${proto.dir}"/>
        <arg value="${proto.dir}/dotnettype/datetime.proto"/>
        <arg value="${proto.dir}/dotnettype/decimal.proto"/>
    </exec>
</project>
